/* 水平间距 */
.header {
  display: flex;
  align-items: center;
  width: 420px;
  height: 200px;
  background: url(data:image/jpeg;base64,) no-repeat;
  background-size: contain;
}
.header .header-image {
  width: 100px;
  height: 100px;
  border-radius: 50%;
  margin-left: 15px;
  border: 2px solid #fff;
}
.header .header-image image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.header .header-nickname {
  display: flex;
  flex-direction: column;
  margin-left: 15px;
}
.header .header-nickname ._h1 {
  font-size: 20px;
  color: #fff;
  margin-bottom: 20rpx;
}
.header .header-nickname ._span {
  margin-bottom: 20rpx;
  font-size: 13px;
  color: rgba(242, 250, 245, 0.8);
}
.body .list .list-item {
  color: #333;
  font-weight: 600;
}
.body .list .list-item .unlogin {
  margin-top: 10px;
  padding: 17px 11px;
  background-color: #fff;
  color: #ed5853;
  text-align: center;
}
.body .list .list-item .item {
  display: flex;
  justify-content: space-between;
  margin-bottom: 10px;
  padding: 17px 11px;
  background-color: #fff;
}
.body .list .list-item .item .item-right {
  display: flex;
}
.body .list .list-item .item .item-right ._span {
  margin-left: 10px;
}

