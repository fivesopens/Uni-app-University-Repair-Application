/* 水平间距 */
page {
  background-color: #f5f5f5;
}
.select_country {
  position: relative;
  width: 100%;
  height: 10%;
  padding: 20px 30px;
  background-color: #FFF;
}
.select_country .uni-icons {
  position: absolute;
  left: 4px;
  top: 20px;
}
.submit {
  display: flex;
  margin-top: 10px;
}
.submit button {
  width: 40%;
  border-radius: 50px;
}
button[type=primary] {
  background-color: #007aff;
}
button[type=default] {
  background-color: #fff;
}

